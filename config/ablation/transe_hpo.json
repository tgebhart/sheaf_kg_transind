{
    "type": "hpo",
    "metadata": {
      "title": "HPO Over FB15K-237 for TransE"
    },
    "pipeline": {
      "stopper": "early",
      "stopper_kwargs": {
        "frequency": 50,
        "patience": 2,
        "relative_delta": 0.002
      },
      "model": "TransE",
      "model_kwargs": {
        "scoring_fct_norm": 2
      },
      "model_kwargs_ranges": {
        "embedding_dim": {
          "type": "int",
          "low": 4,
          "high": 8,
          "scale": "power_two"
        }
      },
      "loss": "CrossEntropyloss",
      "regularizer": "NoRegularizer",
      "optimizer": "adam",
      "optimizer_kwargs": {
        "weight_decay": 0.0
      },
      "optimizer_kwargs_ranges": {
        "lr": {
          "type": "float",
          "low": 0.001,
          "high": 0.1,
          "scale": "log"
        }
      },
      "training_loop": "lcwa",
      "training_kwargs": {
        "num_epochs": 1000
      },
      "training_kwargs_ranges": {
        "label_smoothing": {
          "type": "float",
          "low": 0.001,
          "high": 1.0,
          "scale": "log"
        }
      },
      "evaluator": "RankBasedEvaluator",
      "evaluator_kwargs": {
        "filtered": true
      },
      "evaluation_kwargs": {
        "batch_size": null
      }
    },
    "optuna": {
      "n_trials": 100,
      "metric": "hits@10",
      "direction": "maximize",
      "sampler": "random"
    }
}