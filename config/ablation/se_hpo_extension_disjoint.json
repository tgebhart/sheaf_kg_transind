{
    "type": "hpo",
    "metadata": {
      "title": "HPO Over fb15k237 for se"
    },
    "pipeline": {
      "stopper": "early",
      "stopper_kwargs": {
        "frequency": 50,
        "patience": 2,
        "relative_delta": 0.002
      },
      "model": "se",
      "model_kwargs": {
        "scoring_fct_norm": 2
      },
      "model_kwargs_ranges": {
        "embedding_dim": {
          "type": "int",
          "low": 2,
          "high": 8,
          "scale": "power_two"
        }
      },
      "loss": "MarginRankingLoss",
      "loss_kwargs_ranges": {
        "margin": {
          "type": "float",
          "low": 1.0, 
          "high": 3.0,
          "scale": "linear"
        }
      },
      "optimizer": "adam",
      "optimizer_kwargs": {
        "weight_decay": 0.0
      },
      "optimizer_kwargs_ranges": {
        "lr": {
          "type": "float",
          "low": 0.001,
          "high": 0.1,
          "scale": "log"
        }
      },
      "training_loop": "slcwa",
      "negative_sampler": "BasicNegativeSampler",
      "negative_sampler_kwargs_ranges": {
        "num_negs_per_pos": {
          "type": "int",
          "low": 1,
          "high": 45,
          "q": 1
        }
      },
      "training_kwargs": {
        "num_epochs": 1000,
        "batch_size": 64,
        "label_smoothing": 0.0
      },
      "evaluator": "RankBasedEvaluator",
      "evaluator_kwargs": {
        "filtered": true
      },
      "evaluation_kwargs": {
        "batch_size": 64
      }
    },
    "extension": {
      "diffusion_ranges": {
        "alpha": {
          "type": "float",
          "low": 1e-3,
          "high": 1,
          "scale": "linear",
          "q": 0.111
        },
        "diffusion_iterations": {
          "type": "int",
          "low": 100,
          "high": 10000,
          "scale": "log"
        }
      }
    },
    "optuna": {
      "n_trials": 200,
      "metric": "hits@10",
      "direction": "maximize"
  }
}