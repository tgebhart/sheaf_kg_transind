{
    "type": "hpo",
    "metadata": {
      "title": "HPO for TransR"
    },
    "pipeline": {
      "stopper": "early",
      "stopper_kwargs": {
        "frequency": 50,
        "patience": 2,
        "relative_delta": 0.002
      },
      "model": "transr",
      "model_kwargs": {
        "scoring_fct_norm": 2
      },
      "model_kwargs_ranges": {
        "embedding_dim": {
          "type": "int",
          "low": 4,
          "high": 7,
          "scale": "power_two"
        },
        "relation_dim": {
          "type": "int",
          "low": 3,
          "high": 6,
          "scale": "power_two"
        }
      },
      "loss": "crossentropy",
      "optimizer": "adam",
      "optimizer_kwargs": {
        "weight_decay": 0.0
      },
      "optimizer_kwargs_ranges": {
        "lr": {
          "type": "float",
          "low": 0.001,
          "high": 0.1,
          "scale": "log"
        }
      },
      "training_loop": "lcwa",
      "training_kwargs": {
        "num_epochs": 500,
        "label_smoothing": 0.6,
        "batch_size": 256
      },
      "evaluator": "RankBasedEvaluator",
      "evaluator_kwargs": {
        "filtered": true
      },
      "evaluation_kwargs": {
        "batch_size": 1024
      }
    },
    "optuna": {
      "n_trials": 100,
      "metric": "hits@10",
      "direction": "maximize"
    }
  }