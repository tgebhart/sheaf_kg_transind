{
  "type": "hpo",
  "metadata": {
    "title": "HPO Over FB15K-237 for TransE"
  },
  "pipeline": {
    "stopper": "early",
    "stopper_kwargs": {
      "frequency": 50,
      "patience": 2,
      "relative_delta": 0.002
    },
    "model": "transe",
    "model_kwargs": {
      "scoring_fct_norm": 2,
      "relation_constrainer": "normalize",
      "entity_constrainer": "normalize"
    },
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 4,
        "high": 8,
        "scale": "power_two"
      }
    },
    "loss": "crossentropy",
    "regularizer": "NoRegularizer",
    "optimizer": "adam",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "lcwa",
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 256,
      "label_smoothing": 0.7
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 64
    }
  },
    "extension": {
      "diffusion_ranges": {
        "alpha": {
          "type": "float",
          "low": 1e-3,
          "high": 1,
          "scale": "linear",
          "q": 0.111
        },
        "diffusion_iterations": {
          "type": "int",
          "low": 100,
          "high": 10000,
          "scale": "log"
        }
      }
    },
    "optuna": {
      "n_trials": 50,
      "metric": "hits@10",
      "direction": "maximize"
    }
  }