{
    "type": "hpo",
    "metadata": {
      "title": "HPO Over fb15k237 for se"
    },
    "pipeline": {
      "stopper": "early",
      "stopper_kwargs": {
        "frequency": 50,
        "patience": 2,
        "relative_delta": 0.002
      },
      "model": "se",
      "model_kwargs": {
        "scoring_fct_norm": 2
      },
      "model_kwargs_ranges": {
        "embedding_dim": {
          "type": "int",
          "low": 5,
          "high": 9,
          "scale": "power_two"
        }
      },
      "loss": "NSSAloss",
      "loss_kwargs_ranges": {
        "adversarial_temperature": {
          "type": "float",
          "low": 0.1, 
          "high": 1.0,
          "q": 0.1
        },
        "margin": {
          "type": "float",
          "low": 1, 
          "high": 29,
          "q": 2.0
        }
      },
      "optimizer": "adam",
      "optimizer_kwargs": {
        "weight_decay": 0.0
      },
      "optimizer_kwargs_ranges": {
        "lr": {
          "type": "float",
          "low": 0.001,
          "high": 0.1,
          "scale": "log"
        }
      },
      "training_loop": "slcwa",
      "negative_sampler": "BasicNegativeSampler",
      "negative_sampler_kwargs_ranges": {
        "num_negs_per_pos": {
          "type": "int",
          "low": 1,
          "high": 100,
          "q": 1
        }
      },
      "training_kwargs": {
        "num_epochs": 1000,
        "label_smoothing": 0.0
      },
      "evaluator": "RankBasedEvaluator",
      "evaluator_kwargs": {
        "filtered": true
      },
      "evaluation_kwargs": {
        "batch_size": null
      }
    },
    "optuna": {
      "n_trials": 3,
      "metric": "hits@10",
      "direction": "maximize",
      "sampler": "random"
  }
}